{
  "name": "angular2-skeleton-node",
  "version": "1.0.0",
  "description": "A simple starter Angular2 / Node / Hapi project",
  "scripts": {
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "postinstall": "npm run typings-install",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src/client",
    "start": "npm run server",
    "typings-install": "typings install",
    "watch": "npm run build -- --watch"
  },
